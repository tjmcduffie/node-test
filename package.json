{
  "name": "express-test",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.js",
  "scripts": {
    "start": "export NODE_ENV='production' PORT=8080 && npm run start:prod",
    "start:dev": "export NODE_ENV='development' PORT=3000 && npm run build:prep:builddir && npm run build:client:dev:static && npm run serve:dev",
    "start:int": "export NODE_ENV='integration' PORT=8000 && npm run start:prod",
    "start:prod": "npm run build:prep:builddir && npm run build:prep:symlink && parallelshell 'npm run build:server' 'npm run build:client' && npm run serve",
    "serve": "forever -f -a -o build/log/forever.log -e build/log/error.log build/app/index.js",
    "serve:dev": "export NODE_ENV='development' PORT=3000 && nodemon --delay 1 -V app/index.js --exec babel-node",
    "build:client": "npm run build:img && npm run build:css && webpack",
    "build:client:dev": "export NODE_ENV='development' && npm run build:img && npm run build:css && webpack -d",
    "build:client:dev:auto": "export NODE_ENV='development' && npm run build:img && npm run build:css && webpack -d --watch",
    "build:client:dev:js": "export NODE_ENV='development' && webpack -d",
    "build:client:dev:static": "npm run build:img && npm run build:css",
    "build:css": "mkdir -p build/static && cp -R app/static_src/css build/static",
    "build:img": "mkdir -p build/static && cp -R app/static_src/img build/static",
    "build:prep:builddir": "rm -rf ./build/* && mkdir build/log",
    "build:prep:symlink": "mkdir build/node_modules && cd build && npm install wavy@^1.0.4 && cd -",
    "build:server": "babel app -d build/app",
    "check": "npm run flow && npm run lint && npm run test",
    "db:seed:create": "babel-node ./app/scripts/seedData.js -c",
    "db:seed:remove": "babel-node ./app/scripts/seedData.js -r",
    "db:seed:reset": "babel-node ./app/scripts/seedData.js -R",
    "db:start": "mongod --fork --config ./mongodb.conf --logpath ./build/log/mongodb.log",
    "db:stop": "mongo admin --eval 'db.shutdownServer()'",
    "flow": "flow",
    "lint": "eslint --ext .js,.jsx app/** spec/**",
    "test": "export NODE_ENV='test' && babel-node spec/jasmine.js",
    "test:coverage": "export NODE_ENV='test' && nyc npm run test",
    "test:coverage:check": "export NODE_ENV='test' && nyc --check-coverage npm run test",
    "test:coverage:report": "export NODE_ENV='test' && nyc report",
    "foo:prod": "npm run build:prep:builddir && npm run build:server && npm run serve",
    "foo:dev": "npm run build:prep:builddir && npm run serve:dev"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "ajv": "^5.2.2",
    "axios": "^0.16.2",
    "body-parser": "^1.17.2",
    "bunyan": "^1.8.10",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "css-loader": "^0.28.7",
    "css-modules-require-hook": "^4.1.0",
    "express": "^4.15.2",
    "express-react-views": "^0.10.2",
    "express-routes-versioning": "^1.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "forever": "^0.15.3",
    "helmet": "^3.6.1",
    "history": "^4.6.1",
    "http-server": "^0.10.0",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "mongoose": "^4.11.10",
    "morgan": "^1.8.2",
    "node-uuid": "^1.4.8",
    "parallelshell": "^3.0.1",
    "react": "^15.5.4",
    "react-a11y": "^0.3.4",
    "react-autocomplete": "^1.7.1",
    "react-dom": "^15.6.1",
    "react-modal": "^2.3.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.1.1",
    "react-tabs": "^2.0.0",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.2",
    "rotating-file-stream": "^1.2.2",
    "routr": "^2.1.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony-v2.8.22",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "url-pattern": "^1.0.3",
    "v8flags": "^3.0.0",
    "wavy": "^1.0.4",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2"
  },
  "devDependencies": {
    "args": "^3.0.2",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-strict-mode": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-watch": "^2.0.6",
    "chalk": "^2.0.1",
    "enzyme": "^2.9.1",
    "es2015": "0.0.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.54.1",
    "flow-typed": "^2.1.2",
    "jasmine": "^2.6.0",
    "jasmine-enzyme": "^3.8.2",
    "jasmine-spec-reporter": "^4.1.0",
    "jscodeshift": "^0.3.32",
    "jsdom": "^11.2.0",
    "nodemon": "^1.12.0",
    "nyc": "^10.3.2",
    "react-test-renderer": "^15.6.1",
    "redux-devtools": "^3.4.0",
    "superagent": "^3.5.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5"
  }
}
